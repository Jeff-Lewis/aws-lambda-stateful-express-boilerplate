{
  "name": "aws-lambda-stateful-express-boilerplate",
  "version": "1.0.0",
  "url": "https://github.com/rafaelrpinto/aws-lambda-stateful-express-boilerplate",
  "scripts": {
    "start": "NODE_ENV=development nodemon ./lib/bin/www --exec babel-node",
    "debug": "NODE_ENV=development nodemon --inspect ./lib/bin/www --exec",
    "build": "babel lib -d .dist",
    "deploy": "npm run build && claudia create --handler lambda.handler --deploy-proxy-api --region eu-west-2 --policies policies --memory 1024 --profile default --set-env NODE_ENV=production",
    "update": "npm run build && claudia update",
    "destroy": "claudia destroy"
  },
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "body-parser": "~1.17.1",
    "connect-dynamodb": "^1.0.11",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-session": "^1.15.3",
    "faker": "^4.1.0",
    "hbs": "~4.0.1",
    "helmet": "^3.6.1",
    "memory-cache": "^0.1.6"
  },
  "devDependencies": {
    "aws-sdk": "^2.72.0",
    "babel-cli": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "nodemon": "^1.11.0"
  }
}
